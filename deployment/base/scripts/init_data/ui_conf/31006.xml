            width="autoSize" minWidth="75" minHeight="75"
            height="autoSize" label="fast forward"
            styleName="ffwx8Btn" tooltip="fast forward X8" />
            <Button id="custom1BtnStartScreen" labelPlacement="top"
            v_buttonType="buttonVideoArea" width="autoSize"
            minWidth="75" minHeight="75" height="autoSize"
            v_click="jsCall('customFunc1',myVplayer.vidiunEntry.entryId)"
            label="custom1" styleName="customBtn"
            tooltip="Custom1 tooltip" />
            <Button id="custom2BtnStartScreen" labelPlacement="top"
            v_buttonType="buttonVideoArea" width="autoSize"
            minWidth="75" minHeight="75" height="autoSize"
            v_click="jsCall('customFunc2',myVplayer.vidiunEntry.entryId)"
            label="custom2" styleName="customBtn"
            tooltip="Custom2 tooltip" />
            <Button id="custom3BtnStartScreen" labelPlacement="top"
            v_buttonType="buttonVideoArea" width="autoSize"
            minWidth="75" minHeight="75" height="autoSize"
            v_click="jsCall('customFunc3',myVplayer.vidiunEntry.entryId)"
            label="custom3" styleName="customBtn"
            tooltip="Custom3 tooltip" />
            <Button id="custom4BtnStartScreen" labelPlacement="top"
            v_buttonType="buttonVideoArea" width="autoSize"
            minWidth="75" minHeight="75" height="autoSize"
            v_click="jsCall('customFunc4',myVplayer.vidiunEntry.entryId)"
            label="custom4" styleName="customBtn"
            tooltip="Custom4 tooltip" />
            <Button id="custom5BtnStartScreen" labelPlacement="top"
            v_buttonType="buttonVideoArea" width="autoSize"
            minWidth="75" minHeight="75" height="autoSize"
            v_click="jsCall('customFunc5',myVplayer.vidiunEntry.entryId)"
            label="custom5" styleName="customBtn"
            tooltip="Custom5 tooltip" />
            <Button id="thumbnailBtnStartScreen"
            labelPlacement="top" command="createThumbnail"
            width="autoSize" minWidth="75" minHeight="75"
            height="autoSize" v_buttonType="buttonVideoArea"
            label="Thumb" styleName="thumbnailBtn"
            tooltip="Use current frame as the video thumbnail" />
            <Button id="unmuteBtnStartScreen" command="unMute"
            labelPlacement="top" width="autoSize" height="autoSize"
            label="unmute" minWidth="75" minHeight="75"
            v_buttonType="buttonVideoArea" styleName="unMuteBtn"
            tooltip="unmute sound" />
            <Button id="downloadBtnStartScreen"
            labelPlacement="top" minWidth="75" minHeight="75"
            v_buttonType="buttonVideoArea" width="autoSize"
            height="autoSize" command="download" label="download"
            styleName="downloadBtn" tooltip="download video" />

          </Tile>
        </VBox>
      </screen>
