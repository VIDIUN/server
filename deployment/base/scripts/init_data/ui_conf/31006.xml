<?xml version="1.0" encoding="UTF-8"?>
<layouts isPlaylist="multi">
  <layout layoutId="AppStudioTemplate"
  skinPath="/content/uiconf/vidiun/appstudio/v.1/vdp_skin_aps_v2.swf">

    <Canvas id="topLevelLayout" width="100%" height="100%" styleName="topLevelLayout">
      <HBox id="horizontalPlayerHolder" width="100%" height="100%" styleName="noGapsNoPadding" >
		<VBox id="player" width="100%" height="100%" 
			styleName="noGapsNoPadding">
			<HBox height="30" id="TopTitleScreen" styleName="controllerBoxDark" 
				v_theme_change="true" width="100%">
				<Label height="22" id="movieName" styleName="movieName" 
					width="100%" v_theme_change="true" 
					text="{myVplayer.vidiunEntry.entryName}" />
			</HBox>
			<Canvas height="100%" id="PlayerHolder" 
				styleName="PlayerBackground" width="100%">

				<Module height="0" id="stats" 
					v_path="stats/VidiunStatsModule.swf" width="0"/>
				<VplayerModule height="100%" id="myVplayer" 
					v_path="vplayer/VplayerModule.swf" styleName="centered" 
					width="100%" />
				<screens height="100%" id="screens" styleName="centered" 
					width="100%" v_path="screens/Screens.swf" 
					v_end_screen_id="endScreen" 
					v_end_screen_over_id="endScreen" 
					v_pause_screen_id="pauseScreen" 
					v_pause_screen_over_id="pauseScreen" 
					v_play_screen_id="emptyScreen" 
					v_play_screen_over_id="playScreen" 
					v_start_screen_id="startScreen" 
					v_start_screen_over_id="startScreen" />
				<plymedia height="100%" id="plyMedia" 
					v_path="addons/PlyMediaModule.swfâ€¬" styleName="centered" 
					width="100%"/>
				<waterMark height="100%" id="watermark" 
					v_path="watermarks/WaterMarkLoader.swf" 
					v_watermark_path="http://www.vidiun.com/content/uiconf/vidiun/generic/vidiun_watermark.png" 
					v_watermark_position="bottomLeft" styleName="centered" 
					width="100%" v_watermark_padding="0" />
				<adapTv id="adapTv" height="100%" width="100%" 
					v_adaptv_categories="" v_adaptv_key="" 
					v_pre_listen_to="doPlay" v_pre_dispatch="endPreSession" 
					v_post_listen_to="playerPlayEnd" 
					v_post_dispatch="endPostSession" v_adaptv_zone="default" 
					v_path="adapTv/VdpAdapTvModule.swf" styleName="centered" />
			</Canvas>
			<HBox id="ControllerScreen" width="100%" height="35" 
				styleName="controllerBoxDark" v_theme_change="true">
				<HBox id="ControllerScreenInner" width="100%" height="35" 
					styleName="transparentContainer">

					<Button id="playBtnControllerScreen" command="play" 
						width="25" height="25" 
						v_buttonType="buttonIconControllerArea" 
						styleName="play" tooltip="" />
					<PrevButton id="plPrevButton" direction="prev" 
						v_path="playlist/view/modules/PlaylistNextPrevButton.swf" 
						v_theme_change="true" />
					<VBox id="scrubberContainerControllerScreen" 
						styleName="noGapsNoPadding" width="100%" height="100%">
						<Spacer height="7"/>
						<Scrubber id="scrubberControllerScreen" 
							styleName="scrubberLight" width="100%" 
							v_color_change="true" height="12" 
							v_theme_change="true" />
						<Spacer height="2"/>
						<HBox width="100%" styleName="noGapsNoPadding" >
							<Spacer width="5"/>
							<TimerCountdown id="timerControllerScreen" 
								width="30" height="10" styleName="timer" 
								v_path="timertext/TimerCountdown.swf" 
								timerType="forwards"/>

						</HBox>
					</VBox>
					<VolumeModule width="20" height="15" 
						id="volumeBarControllerScreen" 
						v_path="VolumeBars/VolumeBarB.swf" 
						styleName="volumebarLight" tooltip="change volume" 
						v_theme_change="true" />
					<Button id="fullScreenBtnControllerScreen" 
						command="fullScreen" 
						v_buttonType="buttonIconControllerArea" 
						width="autoSize" height="autoSize" label="fullscreen" 
						styleName="fullScreenBtn" tooltip="Toggle fullscreen" />
					
					<Button id="editBtnControllerScreen" 
						v_buttonType="buttonIconControllerArea" 
						width="autoSize" height="autoSize" 
						v_click="jsCall('gotoEditorWindow',myVplayer.vidiunEntry.entryId);pauseMedia()" 
						label="Edit" styleName="editBtn" v_stats_id="edit"
						tooltip="Remix this video" />
					
					<Button id="uploadBtnControllerScreen" width="autoSize" 
						height="autoSize" v_stats_id="upload"
						v_buttonType="buttonIconControllerArea" 
						v_click="jsCall('gotoContributorWindow',myVplayer.vidiunEntry.entryId);pauseMedia()" 
						label="Upload" styleName="uploadBtn" 
						tooltip="Add additional videos, images or audio files to this video" />
					
					<Button id="shareBtnControllerScreen" 
						v_buttonType="buttonIconControllerArea" 
						command="gigyaButton" label="Share" 
						styleName="shareBtn" tooltip="share with friends" />
					<Button id="flagBtnControllerScreen" 
						v_buttonType="buttonIconControllerArea" 
						width="autoSize" height="autoSize" label="flag" 
						v_click="popup('report')" styleName="flagBtn" 
						tooltip="Report this content as inappropriate" />
					<Button id="creditsBtnControllerScreen" 
						v_buttonType="buttonIconControllerArea" 
						width="autoSize" height="autoSize" label="credits" 
						v_click="popup('credits')" styleName="creditsBtn" 
						tooltip="Users who contributed to this mix" />

					<Button id="ffwx2BtnControllerScreen" command="ffwx2" 
						v_buttonType="buttonIconControllerArea" 
						width="autoSize" height="autoSize" label="fast forward" 
						styleName="ffwx2Btn" tooltip="fast forward X2" />
					<Button id="ffwx4BtnControllerScreen" command="ffwx4" 
						v_buttonType="buttonIconControllerArea" 
						width="autoSize" height="autoSize" label="fast forward" 
						styleName="ffwx4Btn" tooltip="fast forward X4" />
					<Button id="ffwx8BtnControllerScreen" command="ffwx8" 
						v_buttonType="buttonIconControllerArea" 
						width="autoSize" height="autoSize" label="fast forward" 
						styleName="ffwx8Btn" tooltip="fast forward X8" />
					<Button id="custom1BtnControllerScreen" 
						v_buttonType="buttonIconControllerArea" 
						width="autoSize" height="autoSize" 
						v_click="jsCall('customFunc1',myVplayer.vidiunEntry.entryId)" 
						label="custom1" styleName="customBtn" 
						tooltip="Custom1 tooltip" />
					<Button id="custom2BtnControllerScreen" 
						v_buttonType="buttonIconControllerArea" 
						width="autoSize" height="autoSize" 
						v_click="jsCall('customFunc2',myVplayer.vidiunEntry.entryId)" 
						label="custom2" styleName="customBtn" 
						tooltip="Custom2 tooltip" />
					<Button id="custom3BtnControllerScreen" 
						v_buttonType="buttonIconControllerArea" 
						width="autoSize" height="autoSize" 
						v_click="jsCall('customFunc3',myVplayer.vidiunEntry.entryId)" 
						label="custom3" styleName="customBtn" 
						tooltip="Custom3 tooltip" />
					<Button id="custom4BtnControllerScreen" 
						v_buttonType="buttonIconControllerArea" 
						width="autoSize" height="autoSize" 
						v_click="jsCall('customFunc4',myVplayer.vidiunEntry.entryId)" 
						label="custom4" styleName="customBtn" 
						tooltip="Custom4 tooltip" />
					<Button id="custom5BtnControllerScreen" 
						v_buttonType="buttonIconControllerArea" 
						width="autoSize" height="autoSize" 
						v_click="jsCall('customFunc5',myVplayer.vidiunEntry.entryId)" 
						label="custom5" styleName="customBtn" 
						tooltip="Custom5 tooltip" />
					<Button id="thumbnailBtnControllerScreen" 
						command="createThumbnail" width="autoSize" 
						height="autoSize" 
						v_buttonType="buttonIconControllerArea" label="Thumb" 
						styleName="thumbnailBtn" 
						tooltip="Use current frame as the video thumbnail" />

					<Button id="downloadBtnControllerScreen" 
						v_buttonType="buttonIconControllerArea" 
						width="autoSize" height="autoSize" command="download" 
						label="download" styleName="downloadBtn" 
						tooltip="download video" />
				</HBox>
				<Spacer width="3"/>
				<Button id="poweredByControllerScreen" width="32" height="20" 
					v_click="navigate('http://www.vidiun.com')" 
					styleName="vidiunLogoDark" v_theme_change="true"/>
				<Spacer width="3"/>
			</HBox>
		</VBox>
			<VBox width="340" height="100%" id="plContainer" v_hide_in_fullscreen="true" styleName="playlistBgLight" v_theme_change="true">
				<Playlist player="{myVplayer}" visible="false" includeInLayout="false" id="playlistApi" v_path="playlist/PlaylistAPI.swf" width="0" height="0" />

				<Canvas width="100%" height="30" visible="{playlistApi.multiPlaylists}" includeInLayout="{playlistApi.multiPlaylists}">
					<PlaylistTabs id="plTabs" v_theme_change="true" styleName="scrollingTabsStyle" v_path="playlist/view/modules/PlaylistScrollingTabs.swf" height="100%" width="100%" scrollJump="120" buttonWidth="20" />
				</Canvas>
				<VBox width="100%" height="100%" styleName="playlistHolder">
					<List id="listDemo" width="100%" height="100%" useHandCursor="true" styleName="plItemsListDark" v_theme_change="true"
						selectedIndex="{listDemo.dataProvider.getItemIndex(playlistApi.activePlaylist.selectedEntry)}"
						itemClick="playlistApi.playEntry(listDemo.selectedItem)"
						dataProvider="{playlistApi.activePlaylist.entries}"
						v_item_renderer_id="irScreen" variableRowHeight="true" />
				</VBox>
			</VBox>
      </HBox>
	  <Gigya height="100%" id="Gigya"
            v_share_email_body="Hi%2c%3cbr%3eI watched this video and thought you'd enjoy it too %24URL%24 to watch.%3cbr%3e%0d%0a%24sender%24%0d%0a"
            v_share_email_subject="Take a look at this video"
            v_path="addons/Gigya.swf" width="100%" />

    </Canvas>
    <popups>
		<module height="100%" id="report" v_path="popups/Report.swf" width="100%" styleName="report"/>
		<module height="100%" id="credits" v_path="credits/CreditsWindow.swf" width="100%" styleName="credits"/>
    </popups>
