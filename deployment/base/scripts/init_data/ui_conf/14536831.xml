<?xml version="1.0"?>
<uiconf>
	<jars>
		<jar>ScreencastOMaticSidePanel-1.0.1.jar</jar>
		<jar>VidiunSidePanel-1.0.1.jar</jar>
		<jar>VidiunRecorderSkin-1.1.jar</jar>
		<jar>VidiunSkin-1.1.jar</jar>
		<jar>vidiun-java-client-02-04-2012.jar</jar>
		<jar>commons-codec-1.4.jar</jar>
		<jar>commons-httpclient-3.1.jar</jar>
		<jar>commons-logging-1.1.1.jar</jar>
		<jar>log4j-1.2.15.jar</jar>
	</jars>
	<som>
		<captureId>vidiun</captureId>
		<macName>Screen Recorder</macName>
		<sidePanelOnly>true</sidePanelOnly>
		<recorderOptions>
			<skin0>-som.*.item.toggleadvanced.type=Skip</skin0>
			<maxCaptureSec>7200</maxCaptureSec>
		</recorderOptions>
	</som>
	<vidiun>
		<videoBitRate>2000</videoBitRate>
		<category/>
		<conversionProfileId/>
		<submit>
			<title>
				<value>Default Title</value>
				<enabled>true</enabled>
			</title>
		</submit>
		<errorMessages>
			<InvalidVs>
				<starts>Invalid VS</starts>
				<replace>
					<![CDATA[Your session with Vidiun has expired.<br><br>In order to complete the upload please follow the following instructions:<br><ol><li>Refresh the Screencast Recorder page and Reload the Recorder</li><li>Wait for the Recorder to find the last video file</li><li>Press Upload button</li></ol>]]></replace>
			</InvalidVs>
		</errorMessages>
	</vidiun>
</uiconf>